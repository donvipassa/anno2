# Пример файла конфигурации окружения
# Скопируйте этот файл в .env и настройте значения

# URL API для автоматической детекции дефектов
VITE_API_URL=https://visio.weldmarker.ru/detect

# Режим разработки (development | production)
NODE_ENV=development

# Включить детальное логирование ошибок
VITE_DEBUG=false

# Максимальный размер файла изображения (в байтах)
# По умолчанию: 20MB
VITE_MAX_FILE_SIZE=20971520

# Поддерживаемые форматы изображений (через запятую)
VITE_SUPPORTED_FORMATS=image/jpeg,image/png,image/tiff,image/bmp